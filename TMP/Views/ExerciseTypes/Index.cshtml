@using TMP.Domain.Entities.Exercises
@using System.Collections.Generic

@model IEnumerable<ExerciseType>


@{
    ViewBag.Title = "All workout types";
    Layout = "~/Views/Shared/_StndGridLayout.cshtml";
    ViewBag.PageCSS = "allExerciseTypesPage";
}

@section heading {
    <h2>All workout types</h2>
}


<ul>
    @foreach (var exerciseType in Model)
    {
        <li>
            @exerciseType.ExerciseName
        </li>
    }
</ul>

@section rightPanel {
    <ul class="contextLinks">
        <li>
            @Html.ActionLink("Add a new workout", "Add")
        </li>
        <li>
            @Html.ActionLink("Workouts I created", "Mine")
        </li>
    </ul> 
 }


